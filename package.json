{
  "name": "@arctir/devgraph-typescript-client",
  "version": "1.0.6",
  "description": "TypeScript client for teh Devgraph API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arctir/devgraph-typescript-client.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "generate": "openapi-ts",
    "generate:watch": "hey-api openapi-ts --watch",
    "clean": "rimraf dist src/generated",
    "clean:generated": "rimraf src/generated",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "openapi",
    "typescript",
    "api-client",
    "rest",
    "sdk",
    "functions"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.80.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "zod": "^3.22.4"
  }
}
